<div class="modal fade" id=<%= "#{object.id}delModal" %> tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel"><%= t 'confirm.delete.title' %></h4>
      </div>
      <div class="modal-body"><h6><%= t 'confirm.delete.msg', own: object.own.member.name, want: object.want.member.name %></h6></div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal"><%= t 'cancel' %></button>
        <%= link_to t('delete'), object, method: :delete, class:'btn btn-danger' %>
      </div>
    </div>
  </div>
</div>
